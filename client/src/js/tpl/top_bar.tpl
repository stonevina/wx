    <span id="completed_items">
      <a href="#completed" class="fixed_pos">
        <span id="s_completed_count">
          <span>
            <span class="icon-karmaprofile1 karma-icon" id="karma_icon"></span>
            <span class="count">50</span>
          </span>
        </span>
      </a>
    </span>
    <span id="gear_holder" class="menu_icon fixed_pos">
      <img src="/img/img_placeholder.gif" width="26" height="16" class="[object HTMLSpanElement] icon fixed_pos cmp_light_gear"></span>
  </div>
  <div id="quick_find">
    <div>
      <img src="/img/img_placeholder.gif" width="16" height="16" class=" search_icon cmp_light_search">
      <input class="quick_find fixed_pos" type="text text_box" placeholder="快速查找" autocomplete="off" autocorrect="off" spellcheck="false">
      <a href="/Help/Filtering" class="help_icon" target="_blank" style="display: none;">
        <img src="/img/img_placeholder.gif" width="16" height="16" class="cmp_light_query_help"></a>
