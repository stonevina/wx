<div id="top_bar_inner" style="margin-left: 310px;">
  <a href="#" class="left_menu_toggle">
    <img src="/img/img_placeholder.gif" width="20" height="15" class="cmp_light_menu_toggle"></a>
  <div id="logo">
    <a href="#">
      <img src="/img/img_placeholder.gif" width="30" height="30" class="cmp_light_logo"></a>
  </div>
  <div id="top_icons">
    <span id="sync_state_icon"></span>
    <span id="quick_add_task_holder">
      <img src="/img/img_placeholder.gif" width="26" height="16" class=" icon fixed_pos cmp_light_add_task"></span>
